CREATE PROCEDURE PUBLIC."selectRoot"(IN USERNAME VARCHAR(100)) SPECIFIC "selectRoot_10132" LANGUAGE SQL NOT DETERMINISTIC READS SQL DATA NEW SAVEPOINT LEVEL  DYNAMIC RESULT SETS 1 BEGIN ATOMIC DECLARE "selectRootCursor" CURSOR WITH RETURN FOR SELECT "I"."Id" "Id","I"."Title" "Title","I"."DateCreated" "DateCreated","I"."DateModified" "DateModified","I"."MediaType" "MediaType","I"."IsReadOnly" "IsReadOnly","I"."CanRename" "CanRename","I"."CanAddChild" "CanAddChild","I"."Size" "Size","I"."IsRead" "IsRead" FROM PUBLIC."Users" AS "U" JOIN PUBLIC."Items" AS "I" ON "U"."RootId"="I"."Id" WHERE "U"."UserName"=USERNAME FOR READ ONLY;OPEN "selectRootCursor";END
CREATE PROCEDURE PUBLIC."mergeRoot"(IN USERNAME VARCHAR(100),IN ID VARCHAR(100),IN TITLE VARCHAR(100),IN DATECREATED TIMESTAMP(3),IN DATEMODIFIED TIMESTAMP(3),IN MEDIATYPE VARCHAR(100),IN ISREADONLY BOOLEAN,IN CANRENAME BOOLEAN,IN CANADDCHILD BOOLEAN,IN SIZE BIGINT,IN ISVERSIONABLE BOOLEAN) SPECIFIC "mergeRoot_10133" LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL  DYNAMIC RESULT SETS 1 BEGIN ATOMIC DECLARE "mergeRootCursor" CURSOR WITH RETURN FOR SELECT "Id","Title","DateCreated","DateModified","MediaType","IsReadOnly","CanRename","CanAddChild","Size","IsRead" FROM PUBLIC."Items" WHERE "Id"=ID FOR READ ONLY;INSERT INTO PUBLIC."Users"("UserName","RootId","TimeStamp")VALUES(USERNAME,ID,DEFAULT);MERGE INTO PUBLIC."Items" USING(VALUES(ID,TITLE,DATECREATED,DATEMODIFIED,MEDIATYPE,ISREADONLY,CANRENAME,CANADDCHILD,SIZE,ISVERSIONABLE))AS VALS(A,B,C,D,E,F,G,H,I,J)ON "Items"."Id"=VALS.A WHEN MATCHED THEN UPDATE SET "Items"."Title"=VALS.B,"Items"."DateCreated"=VALS.C,"Items"."DateModified"=VALS.D,"Items"."MediaType"=VALS.E,"Items"."IsReadOnly"=VALS.F,"Items"."CanRename"=VALS.G,"Items"."CanAddChild"=VALS.H,"Items"."Size"=VALS.I,"Items"."IsVersionable"=VALS.J WHEN NOT MATCHED THEN INSERT VALUES VALS.A,VALS.B,VALS.C,VALS.D,VALS.E,VALS.F,VALS.G,VALS.H,VALS.I,DEFAULT,DEFAULT,VALS.J,DEFAULT;OPEN "mergeRootCursor";END
CREATE PROCEDURE PUBLIC."getCountOfIdentifier"(IN USERNAME VARCHAR(100)) SPECIFIC "getCountOfIdentifier_10134" LANGUAGE SQL NOT DETERMINISTIC READS SQL DATA NEW SAVEPOINT LEVEL  DYNAMIC RESULT SETS 1 BEGIN ATOMIC DECLARE "getCountOfIdentifierCursor" CURSOR WITH RETURN FOR SELECT COUNT("Id")"Identifier" FROM PUBLIC."Identifiers" WHERE "InUse"=FALSE AND "UserName"=USERNAME FOR READ ONLY;OPEN "getCountOfIdentifierCursor";END

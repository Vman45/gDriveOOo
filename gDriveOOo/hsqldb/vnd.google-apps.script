CREATE PROCEDURE PUBLIC."mergeItem"(IN ID VARCHAR(100),IN TITLE VARCHAR(100),IN DATECREATED TIMESTAMP(3),IN DATEMODIFIED TIMESTAMP(3),IN MEDIATYPE VARCHAR(100),IN ISREADONLY BOOLEAN,IN CANRENAME BOOLEAN,IN CANADDCHILD BOOLEAN,IN SIZE BIGINT,IN ISVERSIONABLE BOOLEAN,OUT NUMROW SMALLINT) SPECIFIC "mergeItem_10137" LANGUAGE SQL NOT DETERMINISTIC MODIFIES SQL DATA NEW SAVEPOINT LEVEL BEGIN ATOMIC DELETE FROM PUBLIC."Children" WHERE "Id"=ID;MERGE INTO PUBLIC."Items" USING(VALUES(ID,TITLE,DATECREATED,DATEMODIFIED,MEDIATYPE,ISREADONLY,CANRENAME,CANADDCHILD,SIZE,ISVERSIONABLE))AS VALS(A,B,C,D,E,F,G,H,I,J)ON "Items"."Id"=VALS.A WHEN MATCHED THEN UPDATE SET "Items"."Title"=VALS.B,"Items"."DateCreated"=VALS.C,"Items"."DateModified"=VALS.D,"Items"."MediaType"=VALS.E,"Items"."IsReadOnly"=VALS.F,"Items"."CanRename"=VALS.G,"Items"."CanAddChild"=VALS.H,"Items"."Size"=VALS.I,"Items"."IsVersionable"=VALS.J WHEN NOT MATCHED THEN INSERT VALUES VALS.A,VALS.B,VALS.C,VALS.D,VALS.E,VALS.F,VALS.G,VALS.H,VALS.I,DEFAULT,DEFAULT,VALS.J,DEFAULT;GET DIAGNOSTICS NUMROW=ROW_COUNT;END
CREATE PROCEDURE PUBLIC."selectChildOn"(IN ID VARCHAR(100),IN URL VARCHAR(250)) SPECIFIC "selectChildOn_10138" LANGUAGE SQL NOT DETERMINISTIC READS SQL DATA NEW SAVEPOINT LEVEL  DYNAMIC RESULT SETS 1 BEGIN ATOMIC DECLARE "selectChildOnCursor" CURSOR WITH RETURN FOR SELECT "I"."Title" "Title","I"."Size" "Size","I"."DateModified" "DateModified","I"."DateCreated" "DateCreated","I"."CanAddChild" "IsFolder",CONCAT(URL,"I"."Id")"TargetURL",FALSE "IsHidden",FALSE "IsVolume",FALSE "IsRemote",FALSE "IsRemoveable",FALSE "IsFloppy",FALSE "IsCompactDisc" FROM PUBLIC."Items" AS "I" JOIN PUBLIC."Children" AS "C" ON "I"."Id"="C"."Id" WHERE "C"."ParentId"=ID FOR READ ONLY;OPEN "selectChildOnCursor";END
CREATE PROCEDURE PUBLIC."selectChildOff"(IN ID VARCHAR(100),IN URL VARCHAR(250)) SPECIFIC "selectChildOff_10139" LANGUAGE SQL NOT DETERMINISTIC READS SQL DATA NEW SAVEPOINT LEVEL  DYNAMIC RESULT SETS 1 BEGIN ATOMIC DECLARE "selectChildOffCursor" CURSOR WITH RETURN FOR SELECT "I"."Title" "Title","I"."Size" "Size","I"."DateModified" "DateModified","I"."DateCreated" "DateCreated","I"."CanAddChild" "IsFolder",CONCAT(URL,"I"."Id")"TargetURL",FALSE "IsHidden",FALSE "IsVolume",FALSE "IsRemote",FALSE "IsRemoveable",FALSE "IsFloppy",FALSE "IsCompactDisc" FROM PUBLIC."Items" AS "I" JOIN PUBLIC."Children" AS "C" ON "I"."Id"="C"."Id" WHERE "C"."ParentId"=ID AND "I"."IsRead"=TRUE FOR READ ONLY;OPEN "selectChildOffCursor";END

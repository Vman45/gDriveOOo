CREATE TRIGGER PUBLIC."OnUpdateItem" INSTEAD OF UPDATE ON PUBLIC."Item" REFERENCING NEW ROW AS NEWROW FOR EACH ROW BEGIN ATOMIC UPDATE PUBLIC."Items" SET "Name"=NEWROW."Name","Size"=NEWROW."Size","ConnectionMode"=NEWROW."ConnectionMode" WHERE "Id"=NEWROW."Id";UPDATE PUBLIC."Capabilities" SET "IsWrite"=NEWROW."IsWrite" WHERE "ItemId"=NEWROW."Id" AND "UserId"=NEWROW."UserId";END
